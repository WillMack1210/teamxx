<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity FindTime.
    -->
    <changeSet id="20251110162605-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="requester_id"
                                 baseTableName="find_time"
                                 constraintName="fk_find_time__requester_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"
                                 />

        <addForeignKeyConstraint baseColumnNames="find_time_id"
                                 baseTableName="rel_find_time__participant"
                                 constraintName="fk_rel_find_time__participant__find_time_id"
                                 referencedColumnNames="id"
                                 referencedTableName="find_time"
                                 />

        <addForeignKeyConstraint baseColumnNames="participant_id"
                                 baseTableName="rel_find_time__participant"
                                 constraintName="fk_rel_find_time__participant__participant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"
                                 />
    </changeSet>
</databaseChangeLog>
